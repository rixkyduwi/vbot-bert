var data={page:page,device:dev,action:"demo-pagination-load-posts"};$.post(ajaxurl,data,function(response){$(".cvf_universal_container").html(response);$(".cvf-pagination-content row.posts").removeClass('loading-state');$('.cvf_universal_container .cvf-pagination-nav .pagination li a.enable').on('click',function(){var page=$(this).attr('p');cvf_load_all_posts(page)})})}
