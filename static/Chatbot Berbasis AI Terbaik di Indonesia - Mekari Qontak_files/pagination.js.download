$($more).click(function(e){e.preventDefault();var $par=$t.find('ul.clients'),$itm=$par.find('.item');$($parent).find('.item:not(.show)').slice(0,18).slideDown().addClass('show');if($itm.length==$par.find('.show').length){$more.closest('.clients-more').fadeOut('slow')}})}})};clientsLoadMore()})(jQuery)